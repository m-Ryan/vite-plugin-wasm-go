{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [],
    "types": [
      "vite/client",
      "node"
    ],
    "outDir": "dist",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": false,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": true,
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {}
  },
  "include": [
    "./src",
  ],
  "exclude": [
    "node_modules"
  ]
}